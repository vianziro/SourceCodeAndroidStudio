<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.wijayasinarkonstruksi.deni.companyprofile">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".WorkActivity"></activity>
        <activity android:name=".ClientActivity"></activity>
        <activity android:name=".PriceActivity"></activity>
        <activity android:name=".HistoriesActivity"></activity>
        <activity android:name=".ContactActivity"></activity>
        <activity android:name=".DeveloperActivity"></activity>
    </application>

</manifest>